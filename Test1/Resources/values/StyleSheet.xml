<?xml version="1.0" encoding="UTF-8" ?>

<resources>
	<style name="largeDate">
		<item name="android:textSize">20dp</item>
		<item name="android:textColor">#000</item>
	</style>
	<style name="largeActiveDate" parent="@style/largeDate">
		<item name="android:textColor">#3A8CF9</item>
	</style>

	<style name="smallDate">
		<item name="android:textSize">8dp</item>
		<item name="android:textColor">#7F828F</item>
	</style>
	<style name="smallActiveDate" parent="@style/smallDate">
		<item name="android:textColor">#4F81BD</item>
	</style>
    <style name="largeWeekendDate" parent="@style/largeDate">
         <item name="android:textColor">#FF0000</item>
	</style>
	<style name="smallWeekendDate" parent="@style/smallDate">
         <item name="android:textColor">#FF0000</item>
	</style>



</resources>
